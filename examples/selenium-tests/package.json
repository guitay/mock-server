{
  "name": "selenium-tests",
  "version": "0.0.0",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "@staticdeploy/mock-server": "^1.10.0",
    "@wdio/cli": "^5.16.15",
    "@wdio/local-runner": "^5.16.15",
    "@wdio/mocha-framework": "^5.16.15",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "node-static": "^0.7.11",
    "npm-run-all": "^4.1.5",
    "react-scripts": "3.3.0",
    "webdriverio": "^5.16.15"
  },
  "scripts": {
    "start:mock-server": "mock-server --watch --delay 1000 --require @babel/register",
    "start:dev-server": "react-scripts start",
    "start": "npm-run-all -p start:*",
    "build": "react-scripts build",
    "serve": "static build",
    "selenium-tests": "wdio wdio.conf.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
